{
  "name": "ev-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",

    "dev:app": "cross-env VITE_TARGET=app vite --port 5173 --mode app",
    "dev:admin": "cross-env VITE_TARGET=admin vite --port 5174 --mode admin",
    "build:app": "cross-env VITE_TARGET=app vite build --outDir dist-app --mode app",
    "build:admin": "cross-env VITE_TARGET=admin vite build --outDir dist-admin --mode admin",
    "preview:app": "vite preview --outDir dist-app --port 5173",
    "preview:admin": "vite preview --outDir dist-admin --port 5174",

    "dev:mpa": "vite --open /src/admin.html",
    "build:mpa": "vite build",
    "preview:mpa": "vite preview",

    "clean": "rimraf dist dist-app dist-admin"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/plots": "^2.6.5",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "rimraf": "^6.0.1"
  }
}
